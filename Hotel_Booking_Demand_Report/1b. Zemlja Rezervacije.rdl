  <DataSets>
    <DataSet Name="DataSource1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        Country.Country_name, Hotel_bookings.Is_canceled, Hotel_bookings.Booking_id, Country.Continent
FROM            Country INNER JOIN
                         Hotel_bookings ON Country.Country_id = Hotel_bookings.Country_id
WHERE        (Hotel_bookings.Is_canceled = 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="Country_name">
          <DataField>Country_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Is_canceled">
          <DataField>Is_canceled</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Booking_id">
          <DataField>Booking_id</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Continent">
          <DataField>Continent</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
