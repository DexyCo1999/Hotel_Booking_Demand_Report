  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>PSP_Hotel_Booking</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>60e8e234-1da0-4caf-9880-01654c749350</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        Hotel_bookings.Is_canceled, Date.Date, Hotel_bookings.Stays_in_weekend_nights, Hotel_bookings.Stays_in_week_nights, Hotel_bookings.Adults, Hotel_bookings.Children, Hotel_bookings.Babies, Date.Date_month, 
                         Date.Date_year, Date_month+'-'+ltrim(str(Date_year)) as Mes_god
FROM            Date INNER JOIN
                         Hotel_bookings ON Date.Date_id = Hotel_bookings.Arrival_date_id
WHERE        (Hotel_bookings.Is_canceled = 0)
order by    date_year, month(date.date)</CommandText>
      </Query>
      <Fields>
        <Field Name="Is_canceled">
          <DataField>Is_canceled</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Stays_in_weekend_nights">
          <DataField>Stays_in_weekend_nights</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Stays_in_week_nights">
          <DataField>Stays_in_week_nights</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Adults">
          <DataField>Adults</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Children">
          <DataField>Children</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Babies">
          <DataField>Babies</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Guests">
          <Value>=Fields!Babies.Value+Fields!Children.Value+Fields!Adults.Value</Value>
        </Field>
        <Field Name="Date_month">
          <DataField>Date_month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Date_year">
          <DataField>Date_year</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Mes_god">
          <DataField>Mes_god</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
