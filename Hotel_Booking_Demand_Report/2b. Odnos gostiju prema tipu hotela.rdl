  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select hotel_type, count(booking_id)
from hotel_bookings hb
inner join hotel h on h.hotel_id = hb.hotel_id
where Is_canceled = 0 
group by hotel_type</CommandText>
      </Query>
      <Fields>
        <Field Name="Hotel_type">
          <DataField>hotel_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
