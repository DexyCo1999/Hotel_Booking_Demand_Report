  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        Date.Date_year, Hotel_bookings.Is_canceled, Hotel_bookings.Booking_id
FROM            Date INNER JOIN
                         Hotel_bookings ON Date.Date_id = Hotel_bookings.Arrival_date_id</CommandText>
      </Query>
      <Fields>
        <Field Name="Date_year">
          <DataField>Date_year</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Is_canceled">
          <DataField>Is_canceled</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Booking_id">
          <DataField>Booking_id</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!Date_year.Value</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=Parameters!GodinaRep.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
