  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        Date.Date_year, Date.month, Hotel_bookings.Stays_in_weekend_nights, Hotel_bookings.Stays_in_week_nights, Date.Date_month
FROM            Date INNER JOIN
                         Hotel_bookings ON Date.Date_id = Hotel_bookings.Arrival_date_id</CommandText>
      </Query>
      <Fields>
        <Field Name="Date_year">
          <DataField>Date_year</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="month">
          <DataField>month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Stays_in_weekend_nights">
          <DataField>Stays_in_weekend_nights</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Stays_in_week_nights">
          <DataField>Stays_in_week_nights</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Stays">
          <Value>=Fields!Stays_in_weekend_nights.Value+Fields!Stays_in_week_nights.Value</Value>
        </Field>
        <Field Name="Date_month">
          <DataField>Date_month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
